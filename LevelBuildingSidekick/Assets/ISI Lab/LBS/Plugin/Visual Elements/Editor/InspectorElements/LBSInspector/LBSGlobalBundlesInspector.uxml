                    <ui:Button tabindex="-1" text="Add as child" display-tooltip-when-elided="true" name="AddChild" tooltip="Create individual assets to assign to the layers" style="flex-grow: 1;" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement style="margin-top: 0; border-left-color: rgba(255, 255, 255, 0); border-right-color: rgba(255, 255, 255, 0); border-top-color: rgba(255, 255, 255, 0); border-bottom-color: rgba(255, 255, 255, 0); background-color: rgba(255, 255, 255, 0.1); border-top-left-radius: 3px; border-bottom-left-radius: 3px; border-top-right-radius: 3px; border-bottom-right-radius: 3px; padding-left: 3px; padding-right: 3px; padding-top: 3px; padding-bottom: 3px; margin-bottom: 4px;">
                <ui:Label tabindex="-1" text="Remove selected" display-tooltip-when-elided="true" />
                <ui:Button tabindex="-1" text="Remove" display-tooltip-when-elided="true" name="RemoveBtn" tooltip="Delete the selected bundle" />
            </ui:VisualElement>
        </ui:VisualElement>
        <GeneralBundlesPanel name="GeneralPanel" style="margin-bottom: 4px;" />
        <CharacteristicsPanel name="CharacteristicsPanel" style="margin-bottom: 4px;" />
    </ui:VisualElement>
</ui:UXML>
